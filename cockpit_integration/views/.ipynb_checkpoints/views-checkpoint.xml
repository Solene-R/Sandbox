<odoo>
  <data>          
          <!-- explicit list view definition -->
      
    <record model="ir.ui.view" id="cockpit_integration.com_list">
      <field name="name">Communication Arrangements list</field>
      <field name="model">cockpit_integration.com_arrangement</field>
      <field name="arch" type="xml">
        <tree>
          <field name="id"/>
          <field name="operation"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="cockpit_integration.action_window">
        <field name="name">Communication Arrangements window</field>
        <field name="res_model">cockpit_integration.com_arrangement</field>
        <field name="view_mode">tree,form</field>
     </record>

          <!-- Top menu item -->
    <menuitem name="Integration cockpit" id="cockpit_integration.menu_root"/>
      
    <!-- menu categories -->
    <menuitem name="Communication Arrangements List" id="cockpit_integration.menu_1" parent="cockpit_integration.menu_root"/>
    
      <!-- actions -->
    <menuitem name="List" id="cockpit_integration.menu_1_list" parent="cockpit_integration.menu_1" action="cockpit_integration.action_window"/>
      
      
